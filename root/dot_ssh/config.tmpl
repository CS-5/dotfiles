{{- if .isMac }}
Include ~/.orbstack/ssh/config
{{- end }}

{{ if not .isDc -}}
Host github.com
  Hostname github.com
  User git
  IdentityFile ~/.ssh/git_personal
  PreferredAuthentications publickey

Host *
  AddKeysToAgent yes
  IdentitiesOnly yes
  Compression yes
{{- if .isMac }}
  UseKeychain yes
{{- end }}
{{ end -}}